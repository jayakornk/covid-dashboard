{
  "name": "jayakornk-covid-dashboard",
  "version": "1.0.0",
  "description": "Simple dashboard to display stats of the coronavirus in Thailand.",
  "repository": "https://github.com/jayakornk/covid-dashboard",
  "author": "Jayakorn Karikan <jayakornk@gmail.com> (https://jayakornk.dev/)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@emotion/cache": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.26",
    "@material-ui/icons": "^4.11.2",
    "apexcharts": "^3.26.1",
    "axios": "^0.21.1",
    "luxon": "^1.26.0",
    "next": "10.0.8",
    "next-pwa": "^5.0.6",
    "react": "17.0.1",
    "react-apexcharts": "^1.3.9",
    "react-dom": "17.0.1",
    "swr": "^0.4.2"
  },
  "devDependencies": {
    "@material-ui/codemod": "^5.0.0-alpha.27",
    "@next/bundle-analyzer": "^10.0.8",
    "@types/luxon": "^1.26.1",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "=4",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --color --cache --fix",
    "*.{json,md,html,css,scss,less}": "prettier --write"
  }
}
